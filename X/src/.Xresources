URxvt.transparent:   false

!#include "/home/matt/.colors/green"
urxvt*smoothResize: true
URxvt.boldFont:xft:Fira Code:Medium:size=16,xft:Source Code Pro:Medium:size=16
URxvt.font:xft:Fira Code:Medium:size=16,xft:Source Code Pro:Medium:size=16
URxvt.letterSpace: -1.8

URxvt.scrollBar: false
URxvt.internalBorder: 5
Xcursor.theme: Adwaita
URxvt.urgentOnBell: true

!infinality font stuffs
!Xft.antialias: 1
!Xft.autohint: 0
!Xft.dpi: 96
!Xft.hinting: 1
!Xft.hintstyle: hintfull
!Xft.lcdfilter: lcddefault
!Xft.rgba: rgb

! hard contrast: *background: #1d2021
